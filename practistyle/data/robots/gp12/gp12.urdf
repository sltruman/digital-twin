<?xml version="1.0" ?>
<robot name="motoman" xmlns:xacro="http://ros.org/wiki/xacro">
    <!-- link list -->
    <link name="base_link">
        <!-- <inertial>
            <mass value="4.0"/>
            <origin rpy="0 0 0" xyz="0.0 0.0 -0.5"/>
            <inertia ixx="0.00443333156" ixy="0.0" ixz="0.0" iyy="0.00443333156" iyz="0.0" izz="0.0072"/>
          </inertial> -->
        <visual>
            <geometry>
                <mesh filename="visual/base_link.stl"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="collision/base_link.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="link_1_s">
        <visual>
            <geometry>
                <mesh filename="visual/link_1_s.stl"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="collision/link_1_s.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="link_2_l">
        <visual>
            <geometry>
                <mesh filename="visual/link_2_l.stl"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="collision/link_2_l.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="link_3_u">
        <visual>
            <geometry>
                <mesh filename="visual/link_3_u.stl"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="collision/link_3_u.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="link_4_r">
        <visual>
            <geometry>
                <mesh filename="visual/link_4_r.stl"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="collision/link_4_r.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="link_5_b">
        <visual>
            <geometry>
                <mesh filename="visual/link_5_b.stl"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="collision/link_5_b.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="link_6_t">
        <visual>
            <geometry>
                <mesh filename="visual/link_6_t.stl"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="collision/link_6_t.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="ee_link"/>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="joint1" type="revolute">
        <parent link="base_link"/>
        <child link="link_1_s"/>
        <origin xyz="0 0 0.450" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="-2.9670" upper="2.9670" effort="926.10" velocity="4.5355"/>
    </joint>
    <joint name="joint2" type="revolute">
        <parent link="link_1_s"/>
        <child link="link_2_l"/>
        <origin xyz="0.155 0 0" rpy="0 0 0" />          <!-- shoulder X -->
        <axis xyz="0 1 0" />
        <limit lower="-1.5708" upper="2.7052" effort="1029.00" velocity="4.0132"/>
    </joint>
    <joint name="joint3" type="revolute">
        <parent link="link_2_l"/>
        <child link="link_3_u"/>
        <origin xyz="0 0 0.614" rpy="0 0 0" />          <!-- l1 -->
        <axis xyz="0 -1 0" />
        <limit lower="-1.4835" upper="2.6179" effort="551.25" velocity="4.5375"/>
    </joint>
    <joint name="joint4" type="revolute">
        <parent link="link_3_u"/>
        <child link="link_4_r"/>
        <origin xyz="0.640 0 0.200" rpy="0 0 0" />      <!-- l2X & l2Z -->
        <axis xyz="-1 0 0" />
        <limit lower="-3.4906" upper="3.4906" effort="89.96" velocity="8.2007"/>
    </joint>
    <joint name="joint5" type="revolute">
        <parent link="link_4_r"/>
        <child link="link_5_b"/>
        <origin xyz="0 0 0" rpy="0 0 0" />              <!-- no -->
        <axis xyz="0 -1 0" />
        <limit lower="-2.6179" upper="2.6179" effort="67.03" velocity="8.1992"/>
    </joint>
    <joint name="joint6" type="revolute">
        <parent link="link_5_b"/>
        <child link="link_6_t"/>
        <origin xyz="0 0 0" rpy="0 0 0" />              <!-- no -->
        <axis xyz="-1 0 0" />
        <limit lower="-7.9412" upper="7.9412" effort="33.30" velocity="12.2143"/>
    </joint>
    <joint name="jointEE" type="fixed">
        <origin xyz="0.100 0 0" rpy="-3.1415926 -1.570796 0"/>       <!-- tool shift -->
        <parent link="link_6_t"/>
        <child link="ee_link"/>
    </joint>
    <!-- end of joint list -->

</robot>
